#version 330 core

in vec2 Texcoord;
in float TexLayer;

out vec4 outColor;

uniform sampler2DArray tex;

void main()
{
	outColor = vec4(fract(gl_FragCoord.x/gl_FragCoord.y), 0, fract(gl_FragCoord.y/gl_FragCoord.x),1); //texColor;
	return;

	vec4 texColor = texture(tex, vec3(Texcoord,TexLayer));
	if(texColor.a < 0.1)
		discard;
}
